	<script>
	$(function() {
		$('.modalBarrier').hide();
		$('.modalBlocker').hide();
		$('#helpModal').hide();
		$('#siteModal').hide();
		$('.helpLink').click( function() {
			$('.modalBarrier').show();
			$('#helpModal').show();
		});
		$('.closeHelp').click( function() {
			$('.modalBarrier').hide();
			$('#helpModal').hide();
		});
		$('#site-select').click(function() {
			$('.modalBarrier').show();
			$('#siteModal').show();
		})
		$('.closeSite').click( function() {
			$('.modalBarrier').hide();
			$('#siteModal').hide();
		});
       $('.closeSite2').click( function() {
			$('.modalBarrier').hide();
			$('#siteModal').hide();
		});
        
        $('.modalBarrier').click( function() {
			$('.modalBarrier').hide();
			$('#siteModal').hide();
            $('#helpModal').hide();
		});

		$('.authBtn').click( function() {
			$('.modalBlocker').show();
			var id = $(this).attr("id");
			document.cookie = "id=" + id;
		});



        $(document).keyup(function(e) {
            if (e.keyCode == 27) {
                $('.modalBarrier').hide();
                $('#siteModal').hide();
                $('#helpModal').hide();
            }
        });
		
	});
    window.onunload = function(){};
	</script>
</head>

<body>

	<div id="banner"></div>

	<div id="authButtons">
	
	
		<p>Choose an authentication method: </p>
		
		
		<a href="https://sso.sandia.gov/Shibboleth.sso/Login?target=https://sso.sandia.gov/idp/Authn/RemoteUserSAML&entityID=https://sts.windows.net/7ccb5a20-a303-498c-b0c1-29007381b574/" class="authBtn">Sandia Azure AD with Seamless SSO OR MS Authenticator on a Sandia phone OR a Yubikey with FIDO2</a>
		
		<a href="https://certauth-sso.sandia.gov/idp/Authn/RemoteUserAssureCert_r?goto=https://sso.sandia.gov/idp/Authn/ExtendedRemoteUserAuth" class="authBtn">HSPD-12 badge OR Yubikey with certs (PIV-D or SNL PIV-C)</a>
		
		<a href="https://sso.sandia.gov/idp/Authn/RemoteUserAssureCryptoCard" class="authBtn">YubiKey one-time password (requires CCHD approval)</a>
		

       <div id="siteModal" class="modal">
            <a href="javascript:void(0)" class="closeSite">close</a>
            <h3 class="modalTitle">Please choose a trusted identity provider to authenticate</h3>
            <div class="site-list">
                
                
                     <a href="https://sso.sandia.gov/Shibboleth.sso/Login?forceAuthn=true&target=https://sso.sandia.gov/idp/Authn/RemoteUserSAML&entityID=https://eidp.llnl.gov&authnContextClassRef=http://idmanagement.gov/icam/2009/12/saml_2.0_profile/assurancelevel3" class="llnl site-select"><span>Lawrence Livermore National Laboratory</span></a>
                
		        
                
                     <a href="https://sso.sandia.gov/Shibboleth.sso/Login?forceAuthn=true&target=https://sso.sandia.gov/idp/Authn/RemoteUserSAML&entityID=https://weblogin.lanl.gov&authnContextClassRef=http://idmanagement.gov/icam/2009/12/saml_2.0_profile/assurancelevel3" class="lanl site-select"><span>Los Alamos National Laboratory</span></a>
                
		        
                
                     <a href="https://sso.sandia.gov/Shibboleth.sso/Login?forceAuthn=true&target=https://sso.sandia.gov/idp/Authn/RemoteUserSAML&entityID=federation:eams-auth.yc.energy.gov:saml2&authnContextClassRef=http://idmanagement.gov/icam/2009/12/saml_2.0_profile/assurancelevel3" class="oneid site-select"><span>OneID</span></a>
                
		        
                <a href="javascript:void(0)" class="closeSite2 sandia site-select"><span>Sandia National Laboratories/Other</span></a>
            </div>
        </div>
	</div>
    <br>
    
    <div id="siteSelection">
    Optionally login in from your home site. <br id="break2"/>
    <br>
    <a class="changeBtn" href="javascript:void(0)" id="site-select">Select Your Site</a>
    </div>
    



